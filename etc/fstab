# <fs>			<mountpoint>	<type>		<opts>					<dump/pass>
/dev/sda1		/boot/efi		vfat		umask=0077					1 2
/dev/sda2		/boot			ext4		noatime						0 0
/dev/sda4		/				ext4		errors=remount-ro			0 0
/dev/sda5		/home			ext4		nodev,nosuid				0 2
none			/tmp			tmpfs		noatime,nodev,nosuid		0 0
/dev/cdrom		/mnt/cdrom		auto		noauto,ro					0 0
LABEL=swap		none			swap		sw							0 0
